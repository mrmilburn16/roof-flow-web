rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // Roof Flow: allow read/write to your company/team when auth is off.
    // When you enable Firebase Auth, change to: allow read, write: if request.auth != null;
    match /companies/c_roofco/teams/t_leadership/{document=**} {
      allow read, write: if true;
    }
  }
}
